<% data = [%w[ID 编号 券种 金额 限制 用户 交易 备注 创建时间 获得时间]] + @vouchers.map { |voucher| [voucher.id, "\t#{voucher.identifier}", voucher.event && voucher.event.name, voucher.event && voucher.event.amount, voucher.event && voucher.event.limitation, voucher.user && voucher.user.name, voucher.trade_id, voucher.remark, voucher.created_at && voucher.created_at.to_s(:db), voucher.obtained_at && voucher.obtained_at.to_s(:db)] } %><%= raw "\357\273\277#{data.map(&:to_csv).join}" %>