<% if flash[:notice] %>
    <div class="alert alert-info">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
      <strong><%= flash[:notice] %></strong>
    </div>
<% end %>
<div class="flash_show" style="display: none">
  <div class="alert alert-info">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
    <strong></strong>
  </div>
</div>
